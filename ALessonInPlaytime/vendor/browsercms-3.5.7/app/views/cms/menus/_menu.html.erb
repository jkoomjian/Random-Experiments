<div id="<%= css_id %>">
<% unless items.empty?
%>  <ul class="<%= css_class %>">
  <li class="nav-logo showmobile"><a href="/"></a></li>
<% items.each_with_index do |item, i|
      %><%= render :partial => "/cms/menus/menu_item", :object => item, :locals => { :depth => 1, :position => i + 1, :item_count => items.length }
      %><% end 
%>  </ul>
<% end
%></div>
